generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Progresso {
  id        Int      @id @default(autoincrement())
  usuario   String   @unique
  criadoEm  DateTime @default(now())
  fase1     Int      @default(0)
  fase2     Int      @default(0)
  fase3     Int      @default(0)
  total     Int      @default(0)
}










