generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}



model Progresso {
  usuario String @id @map("user") // ‚Üê ESSENCIAL
  fase1   Int
  fase2   Int
  fase3   Int
  total   Int
}

model Token {
  id    Int    @id @default(autoincrement())
  token String @unique
}














