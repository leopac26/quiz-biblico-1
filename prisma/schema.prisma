generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Progresso {
  id     Int    @id @default(autoincrement())
  usuario String @unique
  fase1   Int
  fase2   Int
  fase3   Int
  total   Int
}


model Token {
  id    Int    @id @default(autoincrement())
  token String @unique
}
